PROJECT_NAME           = @PROJECT_NAME@
PROJECT_NUMBER         = @PROJECT_VERSION@
PROJECT_BRIEF          = "A header only, dependency-free deep learning framework in C++11"
STRIP_FROM_PATH        = @PROJECT_SOURCE_DIR@ \
                         @PROJECT_BINARY_DIR@
OUTPUT_LANGUAGE        = English
INPUT                  = @PROJECT_SOURCE_DIR@/tiny_dnn \
                         @PROJECT_SOURCE_DIR@/examples \
                         @PROJECT_SOURCE_DIR@/docs
FILE_PATTERNS          = *.h *.md
RECURSIVE              = YES
IMAGE_PATH             = @PROJECT_SOURCE_DIR@/docs
USE_MDFILE_AS_MAINPAGE = @PROJECT_SOURCE_DIR@/docs/readme.md
JAVADOC_AUTOBRIEF      = YES
GENERATE_HTML          = YES
GENERATE_LATEX         = NO
OUTPUT_DIRECTORY       = @PROJECT_BINARY_DIR@/docs
EXCLUDE_PATTERNS       = */caffe.pb.h
